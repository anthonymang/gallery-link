<div class='row'>

    <div class='col-2 search-left'></div>
    
    <div class='col-8 search-results'>
        <br>
    <h1 class='results-header'>Galleries</h1>

<% galleries.forEach(r =>{ %>
    <div class= 'individual-results'>
        <h4><a href="galleries/<%= r.id %>"><%=r.name%></a></h4>
        <p><%= r.description %></p>
        </div>
<% }) %>


</div>

<div class='col-2 search-right'></div>


</div>